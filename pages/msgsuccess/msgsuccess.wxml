<!--pages/msgsuccess/msgsuccess.wxml-->
<view class="page">
    <view class="weui-msg">
       
        <icon type="success" size="70" />
        <view class="weui-msg__text-area">
            <text class="weui-msg__title">预约成功</text>
            <view class="weui-msg__desc">预约成功，请在规定时间内准时到达上课地点，如有疑问，请电话联系我们。</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button href="javascript:history.back();" bindtap='lookRecords' type = "primary" class="weui-btn weui-btn_primary">查看约课</button>
                <button href="javascript:history.back();" bindtap='chooseCourses' class="weui-btn weui-btn_default">继续选课</button>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__links">
                    <!-- <view href="javascript:void(0);" class="weui-footer__link">底部链接文本</view> -->
                </view>
                <view class="weui-footer__text">Copyright © 2019 三只小兔</view>
            </view>
        </view>
    </view>
</view>
